  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "README.md",
    "dist",
    "package.json"
  ],
  "scripts": {
    "dev": "pnpm run stub",
    "stub": "unbuild",
    "build": "unbuild && pnpm run play:build",
